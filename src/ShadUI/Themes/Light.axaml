<ResourceDictionary
    xmlns="https://github.com/avaloniaui"
    xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml">

    <!-- basic colors -->
    <Color x:Key="ForegroundColor">#18181B</Color>
    <Color x:Key="BackgroundColor">Transparent</Color>
    <Color x:Key="MutedColor">#E4E4E4</Color>
    <Color x:Key="CardBackgroundColor">#1218181B</Color>
    <Color x:Key="WindowBackgroundColor">#FFFFFF</Color>
    <Color x:Key="BorderColor">#E4E4E7</Color>

    <Color x:Key="PrimaryColor">#09090B</Color>
    <Color x:Key="PrimaryColor75">#BF09090B</Color>
    <Color x:Key="PrimaryForegroundColor">#FAFAFA</Color>

    <Color x:Key="SecondaryColor">#18181B</Color>
    <Color x:Key="SecondaryColor75">#BF18181B</Color>
    <Color x:Key="SecondaryForegroundColor">#FAFAFA</Color>

    <Color x:Key="DestructiveColor">#DC2626</Color>
    <Color x:Key="DestructiveColor75">#BFDC2626</Color>
    <Color x:Key="DestructiveForegroundColor">#FAFAFA</Color>

</ResourceDictionary>
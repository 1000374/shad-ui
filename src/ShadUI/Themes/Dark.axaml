<ResourceDictionary
    xmlns="https://github.com/avaloniaui"
    xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml">

    <!-- basic colors -->
    <Color x:Key="ForegroundColor">#FAFAFA</Color>
    <Color x:Key="ForegroundLeadColor">#A1A1AA</Color>

    <Color x:Key="BackgroundColor">Transparent</Color>
    <Color x:Key="MutedColor">#A0A0A9</Color>
    <Color x:Key="BorderColor">#353535</Color>
    <Color x:Key="ErrorColor">#C82323</Color>

    <Color x:Key="CardBackgroundColor">#09090B</Color>
    <Color x:Key="SidebarBackgroundColor">#09090B</Color>
    <Color x:Key="TitleBarBackgroundColor">#09090B</Color>
    <Color x:Key="WindowBackgroundColor">#0A0A0D</Color>
    
    <Color x:Key="SwitchBackgroundColor">#27272A</Color>
    <Color x:Key="SwitchForegroundColor">#09090B</Color>

    <Color x:Key="PrimaryColor">#FAFAFA</Color>
    <Color x:Key="PrimaryColor75">#BFFAFAFA</Color>
    <Color x:Key="PrimaryColor10">#19FAFAFA</Color>
    <Color x:Key="PrimaryForegroundColor">#18181B</Color>

    <Color x:Key="SecondaryColor">#2C2C30</Color>
    <Color x:Key="SecondaryColor75">#BF2C2C30</Color>
    <Color x:Key="SecondaryForegroundColor">#FAFAFA</Color>

    <Color x:Key="DestructiveColor">#C82323</Color>
    <Color x:Key="DestructiveColor75">#BFC82323</Color>
    <Color x:Key="DestructiveForegroundColor">#FAFAFA</Color>

    <Color x:Key="OutlineColor">#BF27272A</Color>
    <Color x:Key="GhostColor">#BF27272A</Color>
</ResourceDictionary>